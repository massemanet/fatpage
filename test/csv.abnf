file        = 1*record-eol

record-eol  = record ( 1*eol /  EOF )         : Y1.

eol         = crlf / cr / lf

record      = field *comma-field              : [Y1|Y2].

comma-field = comma field                     : Y2.

field       = quoted / unquoted

quoted      = *WS dquote *(uqtext / comma) dquote *WS    : merge(Y3).

unquoted    = *uqtext                         : merge(Y).

uqtext      = unicodeish / *WS / doubledq

doubledq    = dquote dquote                   : Y1.

crlf        = cr lf

cr          = %x0A

lf          = %x0D

WS          = %x09 / %x20

dquote      = %x22

comma       = %x2C

unicodeish  = %x21 / %x23-2B / %x2D-7E / %x80-7FF / %x800-FFFF / %x10000-10FFFF
